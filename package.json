    "@vercel/speed-insights": "^1.0.11",
    "antd": "^5.18.0",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "docx": "^8.5.0",
    "framer-motion": "^11.2.10",
    "gsap": "^3.12.5",
    "mammoth": "^1.7.2",
    "mapbox-gl": "^3.4.0",
    "next": "^14.2.3",
