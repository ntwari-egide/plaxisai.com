    "@vercel/speed-insights": "^1.0.10",
    "antd": "^5.5.2",
    "axios": "^1.6.8",
    "clsx": "^1.2.1",
    "docx": "^8.5.0",
    "framer-motion": "^10.16.14",
    "mammoth": "^1.7.2",
    "mapbox-gl": "^3.1.2",
    "next": "^13.4.5",
    "nodemailer": "^6.9.1",
